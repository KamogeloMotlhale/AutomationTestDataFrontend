@page
@model Automation_Test_Data_App.Pages.PolicyServicing.ChangeLifeData.IndexModel
@{
}

<br>
<h2>Change Life Assured Data </h2>
<a class='btn btn-primary btn-sm' href='ChangeLifeData/Create'>New Life Assured Details</a>
<table class="table">
	<thead>
		<tr>
			<th>ID<th/>
			<th>Title<th/>
			<th>Surname<th/>
			<th>Marita Status<th/>
