@page
@model Automation_Test_Data_App.Pages.PolicyServicing.TerminateRolePlayer.IndexModel
@{
}

<br>
<h2>Terminate Role</h2>
<a class='btn btn-primary btn-sm' href='Create'>Terminate/Terminate Role</a>
<table class="table">
	<thead>
		<tr>
			<th>ID<th/>
			<th>RoleType<th/>
			<th>Scenario_ID<th/>
			


	</thead>
	<tbody>
		@foreach (var item in Model.ListTerminateinfo)
