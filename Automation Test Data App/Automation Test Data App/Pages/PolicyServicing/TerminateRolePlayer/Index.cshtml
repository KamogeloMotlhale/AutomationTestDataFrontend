				<td>@item.RoleType</td>
				<td></td>
				<td>@item.Scenario_ID</td>
				
				<td></td>
				<td>
					<a class="btn btn-primary btn-sm" href="/PolicyServicing/TerminateRolePlayer/Edit?id=@item.id">Edit</a>
