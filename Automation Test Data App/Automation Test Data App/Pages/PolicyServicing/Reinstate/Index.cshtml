@page
@model Automation_Test_Data_App.Pages.PolicyServicing.Reinstate.IndexModel
@{
}

<br>
<h2>Reinstate Info Policy Component</h2>
<a class='btn btn-primary btn-sm' href='Reinstate/Create'>Reinstate Policy</a>
<table class="table">
	<thead>
		<tr>
			<th>ID<th/>
			<th>Date<th/>
			<th>Reason<th/>
			<th>Action<th/>



	</thead>
	<tbody>
		@foreach (var item in Model.ListReinstate)
		{

			<tr>
				<td>@item.id</td>
				<td></td>
				<td>@item.Date</td>
				<td></td>
				<td>@item.Reason</td>

				<td>
					<a class="btn btn-primary btn-sm" href="/ReinstateEdit?id=@item.id">Edit</a>
					<a class="btn btn-danger btn-sm" href="/PolicyServicing/Reinstate/Delete?id=@item.id" title="Delete" class="delete" onclick="return confirm('Are you sure you want to delete this item')">Delete</a>

				</td>
				
				
			</tr>
		}

	</tbody>
</table>
