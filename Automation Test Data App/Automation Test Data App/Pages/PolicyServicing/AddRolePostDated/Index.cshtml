@page
@model Automation_Test_Data_App.Pages.AddRolePostDated.IndexModel
@{
}

<br>
<h2>Create PostDated RolePlayer </h2>
<a class='btn btn-primary btn-sm' href='Create'>PostDated RolePlayer</a>
<table class="table">
	<thead>
		<tr>
			<th>ID<th/>
			<th>Title<th/>
			<th>First Name<th/>
			<th>Surname<th/>
			<th>Initials<th/>
			<th>DOB<th/>
			<th>Gender<th/>
			<th>ID number<th/>
			<th>Relationship<th/>
			<th>Comm date<th/>
			<th>Cover Amount<th/>
			<th>Action<th/>
		</tr>
	</thead>
	<tbody>
		@foreach (var item in Model.ListAddRolePostDated)
		{

			<tr>
				<td>@item.id</td>
						<td></td>
				<td>@item.Title</td>
					<td></td>
				<td>@item.First_Name</td>
					<td></td>
				<td>@item.Surname</td>
					<td></td>
				<td>@item.Initials</td>
					<td></td>
				<td>@item.DOB</td>
					<td></td>
				<td>@item.Gender</td>
					<td></td>
				<td>@item.ID_number</td>
					<td></td>
				<td>@item.Relationship</td>
					<td></td>
				<td>@item.Comm_date</td>
				<td></td>
				<td>@item.CoverAmount</td>
				<td></td>
				<td>

					<a class="btn btn-primary btn-sm" href="/PolicyServicing/AddRolePostDated/Edit?id=@item.id">Edit</a>
					<a class="btn btn-danger btn-sm" href="/PolicyServicing/AddRolePostDated/Delete?id=@item.id" title="Delete" class="delete" onclick="return confirm('Are you sure you want to delete this item')">Delete</a>


				</td>
				

			</tr>
		}

	</tbody>
</table>
