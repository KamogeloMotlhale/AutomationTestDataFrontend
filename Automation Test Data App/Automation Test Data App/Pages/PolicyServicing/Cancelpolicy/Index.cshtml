@page
@model Automation_Test_Data_App.Pages.PolicyServicing.Cancelpolicy.IndexModel
@{
}

<br>
<h2>Cancel Policy Data</h2>
<a class='btn btn-primary btn-sm' href='Create'>Cancel Policy</a>
<table class="table">
	<thead>
		<tr>
			<th>ID<th/>
			<th>Policy Number<th/>
			<th>Termination Reason<th/>
			<th>Termination Date<th/>
			<th>Action<th/>



	</thead>
	<tbody>
		@foreach (var item in Model.ListCancelpolicy)
		{

			<tr>
				<td>@item.id</td>
				<td></td>
				<td>@item.PolicyNo</td>
				<td></td>
				<td>@item.TerminationReason</td>
				<td></td>
				<td>@item.TerminationDate</td>
				<td></td>
				<td>
					<a class="btn btn-primary btn-sm" href="/PolicyServicing/Cancelpolicy/Edit?id=@item.id">Edit</a>
					<a class="btn btn-danger btn-sm" href="/PolicyServicing/Cancelpolicy/Delete?id=@item.id" title="Delete" class="delete" onclick="return confirm('Are you sure you want to delete this item')">Delete</a>

				</td>
				
				
			</tr>
		}

	</tbody>
</table>
